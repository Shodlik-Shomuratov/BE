{
  "name": "product-service",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "invoke:getProductsList": "serverless invoke local --function getProductsList",
    "invoke:getProductsById": "serverless invoke local --function getProductsById --path src/functions/getProductsById/invoke.json",
    "invoke:createProduct": "serverless invoke local --function createProduct --path src/functions/createProduct/invoke.json",
    "test": "jest",
    "populateDB": "ts-node src\\utils\\populateDataBase.ts"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^2.5.3",
    "@middy/http-json-body-parser": "^2.5.3"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-typescript": "^7.18.6",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^29.0.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.14.25",
    "babel-jest": "^29.0.3",
    "esbuild": "^0.14.11",
    "jest": "^29.0.3",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.0.0",
    "serverless-auto-swagger": "^2.8.2",
    "serverless-esbuild": "^1.23.3",
    "ts-jest": "^29.0.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
